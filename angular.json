{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "official-site",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "official-site": {
      "projectType": "application",
      "root": "apps/official-site",
      "sourceRoot": "apps/official-site/src",
      "prefix": "angular-timeline",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/official-site",
            "index": "apps/official-site/src/index.html",
            "main": "apps/official-site/src/main.ts",
            "polyfills": "apps/official-site/src/polyfills.ts",
            "tsConfig": "apps/official-site/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/official-site/src/favicon.ico",
              "apps/official-site/src/assets"
            ],
            "styles": ["apps/official-site/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/official-site/src/environments/environment.ts",
                  "with": "apps/official-site/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "official-site:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "official-site:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "official-site:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/official-site/src/**/*.ts",
              "apps/official-site/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/official-site"],
          "options": {
            "jestConfig": "apps/official-site/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "official-site-e2e": {
      "root": "apps/official-site-e2e",
      "sourceRoot": "apps/official-site-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/official-site-e2e/cypress.json",
            "tsConfig": "apps/official-site-e2e/tsconfig.e2e.json",
            "devServerTarget": "official-site:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "official-site:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/official-site-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "angular-timeline": {
      "projectType": "library",
      "root": "libs/angular-timeline",
      "sourceRoot": "libs/angular-timeline",
      "prefix": "bs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/angular-timeline/tsconfig.lib.json",
            "project": "libs/angular-timeline/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/angular-timeline/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular-timeline/**/*.ts",
              "libs/angular-timeline/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/angular-timeline"],
          "options": {
            "jestConfig": "libs/angular-timeline/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/angular-timeline/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/angular-timeline",
            "config": {
              "configFolder": "libs/angular-timeline/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "angular-timeline-e2e": {
      "root": "apps/angular-timeline-e2e",
      "sourceRoot": "apps/angular-timeline-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-timeline-e2e/cypress.json",
            "tsConfig": "apps/angular-timeline-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular-timeline:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "angular-timeline:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/angular-timeline-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
